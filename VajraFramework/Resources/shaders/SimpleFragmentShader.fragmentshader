// precision mediump float;

varying vec4 lightIntensity;
varying vec4 extraVariable;
varying vec2 uvCoords;
uniform sampler2D myTextureSampler;

void main()
{
	vec4 color2;
	// gl_FragColor = vec4(1.0, 1.0, 0.0, 0.0);
	// gl_FragColor = extraVariable;
	// gl_FragColor = vec4(uvCoords.x, uvCoords.y, 0.0, 1.0);
	gl_FragColor = vec4(texture2D(myTextureSampler, uvCoords).rgb, 1.0) * lightIntensity;
}
