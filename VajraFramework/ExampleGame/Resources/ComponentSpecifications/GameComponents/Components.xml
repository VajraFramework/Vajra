	<component name="AiPerception" includePath="ExampleGame/Components/GameScripts/Ai/AiPerception.h">
	</component>
	
	<component name="AiRoutine" includePath="ExampleGame/Components/GameScripts/Ai/AiRoutine.h">
	</component>
	
	<component name="AiKnowledge" includePath="ExampleGame/Components/GameScripts/Ai/AiKnowledge.h">
	</component>
	
	<component name="EnemyUnit" includePath="ExampleGame/Components/GameScripts/Units/EnemyUnit.h">
	</component>
	
	<component name="PlayerUnit" includePath="ExampleGame/Components/GameScripts/Units/PlayerUnit.h">
	</component>
	
	<component name="ShadyCamera" includePath="ExampleGame/Components/ShadyCamera/ShadyCamera.h">
		<property name="MoveTo">
			<field name="x" datatype="float"></field>
			<field name="y" datatype="float"></field>
			<field name="z" datatype="float"></field>
