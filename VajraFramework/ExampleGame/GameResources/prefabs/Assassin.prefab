<prefab>
	<gameobject>
		<component name="MeshRenderer">
			<property name="InitMesh">
				<field>models/SD_Assassin_mesh.model</field>
			</property>
		</component>

		<component name="BakedSkeletalAnimation">
			<property name="AddAnimationClip">
				<field>animations/SD_Assassin_mesh.skeletalanimation#idle</field>
				<field>0.25</field>
				<field>true</field>
			</property>
			<property name="AddAnimationClip">
				<field>animations/SD_Assassin_mesh.skeletalanimation#walking</field>
				<field>1.0</field>
				<field>true</field>
			</property>
			<property name="AddAnimationClip">
				<field>animations/SD_Assassin_mesh.skeletalanimation#special_posing</field>
				<field>0.25f</field>
				<field>false</field>
			</property>
			<property name="AddAnimationClip">
				<field>animations/SD_Assassin_mesh.skeletalanimation#special_action</field>
				<field>0.25f</field>
				<field>false</field>
			</property>
			<property name="AddAnimationClip">
				<field>animations/SD_Assassin_mesh.skeletalanimation#special_end</field>
				<field>1.5f</field>
				<field>false</field>
			</property>
			<property name="AddAnimationClip">
				<field>animations/SD_Assassin_mesh.skeletalanimation#death</field>
				<field>1.0f</field>
				<field>false</field>
			</property>
			<property name="PlayAnimationClip">
				<field>idle</field>
			</property>
		</component>

		<component name="Transform">
			<property name="Scale">
				<field>0.07</field>
			</property>
		</component>
		
		<component name="AudioSource">
			<property name="SetSourceIs3D">
				<field>false</field>
			</property>
			<property name="SetPlayOnlyWhenVisible">
				<field>true</field>
			</property>
			<property name="LoadAudioClip">
				<field>death</field>
				<field>audio/SD_SFX_AssassinDeath_01.wav</field>
			</property>
			<property name="LoadAudioClip">
				<field>specialStart</field>
				<field>audio/SD_SFX_AssassinAttack_01.wav</field>
			</property>
			<property name="LoadAudioClip">
				<field>specialHit</field>
				<field>audio/SD_SFX_AssassinAttackHit_01.wav</field>
			</property>
			<property name="LoadAudioClip">
				<field>walk</field>
				<field>audio/SD_SFX_AssassinFootstep_01.wav</field>
			</property>
		</component>
		
		<component name="GridNavigator">
		</component>
		
		<component name="Assassin">
			<property name="SetDashEffect">
				<field>assassindash.prefab</field>
			</property>
			<property name="SetSpecialHitEffect">
				<field>assassinhit.prefab</field>
			</property>
		</component>

		<component name="UnitAnimationManager">
		</component>

		<component name="Decal">
			<property name="SetDecalImages">
				<field>blank.png</field>
				<field>1.0</field>
				<field>assassin_icon.png</field>
				<field>2.0</field>
			</property>
		</component>

	</gameobject>
</prefab>

